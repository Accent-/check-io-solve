    "def checkio(data):\n",
    "    length = len(data) //2\n",
    "    buf=sorted(data)\n",
    "    if len(data) % 2 == 1:\n",
    "        return buf[length]\n",
    "    elif len(data) % 2 ==0:\n",
    "        return (buf[length -1] + buf[length]) / 2\n",
    "    else:\n",
    "        pass"
   ]
